import org.junit.Assert;
import org.junit.Test;
import org.junit.runner.RunWith;
import org.junit.runners.Parameterized;

import java.util.Arrays;
import java.util.Collection;

@RunWith(Parameterized.class)
public class MassTestArray2 {
        private int[] in;
        private boolean result;

        @Parameterized.Parameters
    public static Collection<Object[]> data(){
            return Arrays.asList(new Object[][]{
                    {new int[]{1, 1, 1, 4, 4, 1, 4, 4},true},
                    {new int[]{1, 1, 1, 1, 1, 1},false},
                    {new int[]{4, 4, 4, 4},false},
                    {new int[]{1, 4, 4, 1, 1, 4, 3},false}
            });
        }

    public MassTestArray2(int[] in, boolean result) {
        this.in = in;
        this.result = result;
    }

    @Test
    public void massTestarrayOnly1and4(){
        Assert.assertEquals(result, MyClass.arrayOnly1and4(in));
    }
}

